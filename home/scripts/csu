echo ----------------------------------------------------------------- TESTING
echo Testing...
echo ----------------------------------------------------------------- IP
ifconfig -a |grep '192.168'
echo ----------------------------------------------------------------- ROUTER
ping -c 2 router
echo ----------------------------------------------------------------- WAN
ping -c 2 www.csu.edu.au
echo ----------------------------------------------------------------- ETHERNET
echo Restarting...
sudo ifconfig eno1 down
sudo ifconfig eno1 up
echo ----------------------------------------------------------------- IP
ifconfig -a |grep '192.168'
echo ----------------------------------------------------------------- ROUTER
ping -c 2 router
echo ----------------------------------------------------------------- WAN
ping -c 2 www.csu.edu.au
